// Este é o seu arquivo de schema do Prisma,
// saiba mais sobre ele na documentação: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id        String   @id @default(uuid())
  email String @unique
  nome  String
  senha String
}
